cat d2 | sed -e 's/<a/\n/g' -e 's/<\/a>/\n/g' | grep href | awk -F'"' '{split($6,a," ");if(index($4,"db//")==0 && length(a[3])>0){split($4,b,"/");print b[5] "," $4 "," a[3] "," a[4] "," $8;}}' > d3


